#include <bits/stdc++.h>
using namespace std;

int missingNumber(int arr[],int start, int end)
{
    if(start>end)
    return end+1;
    
    if(start != arr[start])
        return start;
    
    int mid = (start+end)/2;
    
    if(arr[mid] == mid)
        return missingNumber(arr,mid+1,end);
    
    return missingNumber(arr,start,mid);
}

int main()
{
    int arr[10],n;
    cin>>n;
    for(int i=0;i<n;i++)
    cin>>arr[i];
    cout<<missingNumber(arr,0,n-1);
}
