#include <iostream>
using namespace std;
struct node
{
    int data;
    struct node *next;
};
struct node *start=NULL;
void create(int a)
{
    struct node *new_node,*ptr;
    new_node=new node();
    new_node->data=a;
    new_node->next=NULL;
    if(start==NULL)
        start=new_node;
    else
    {
        ptr=start;
        while(ptr->next!=NULL)
            ptr=ptr->next;
        ptr->next=new_node;
    }
}
void display()
{
    struct node *ptr=start;
    while(ptr->next!=NULL)
    {
        cout<<ptr->data<<"->";
        ptr=ptr->next;
    }
    cout<<ptr->data;
}
void sort()
{
    int temp;
    struct node *ptr1,*ptr2;
    ptr1=start;
    while(ptr1->next!=NULL)
    {
        ptr2=ptr1->next;
        while(ptr2!=NULL)
        {
            if(ptr1->data>ptr2->data)
                {
                    swap(ptr1->data,ptr2->data);
                }
            ptr2=ptr2->next;    
        }
        ptr1=ptr1->next;
    }   
    
}
int main()
{
    int a,n;
    cin>>n;
    while(n--)
    {
        cin>>a;
        create(a);
    }
display();
sort();
cout<<endl;
display();
return 0;
}
