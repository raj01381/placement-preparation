#include <iostream>
#include <algorithm>

using namespace std;
void sortinwave(int arr[10], int n);

int main()
{
  int arr[10],n,i;
  cin>>n;
  for(i=0;i<n;i++)
    cin>>arr[i];
  for(i=0;i<n;i+=2)
  sortinwave(arr,n);
  for(i=0;i<n;i+=2)
  cout<<arr[i]<<" ";
}

void sortinwave(int arr[],int n)
{
  sort(arr,arr+n);
  
  for(int i=0;i<n-1;i+=2)
    swap(&arr[i],&arr[i+1]);
}

void swap(int *a, int *b)
{
  int temp = *a;
  *a = *b;
  *b = temp;
}
