#include <iostream>
using namespace std;
struct node
{
    int data;
    struct node * next;
};
struct node * start=NULL;
void create(int n)
{
    struct node *new_node,*ptr;
    new_node=new node();
    new_node->data=n;
    new_node->next=NULL;
    if(start==NULL)
        start=new_node;
    else
    {
        ptr=start;
        while(ptr->next!=NULL)
            ptr=ptr->next;
        ptr->next=new_node;
    }
}
void display()
{
    struct node *ptr=start;
    while(ptr->next!=NULL)
    {
        cout<<ptr->data<<"->";
        ptr=ptr->next;
    }
    cout<<ptr->data;
}
int main()
{
    int a,n;
    cin>>n;
    while(n--)
    {
        cin>>a;
        create(a);
    }
    display();
	return 0;
}
