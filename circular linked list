#include <stdio.h>
#include <malloc.h>
#include<stdlib.h>
struct node
{
    int data;
    struct node* next;
};
struct node *START=NULL;
void create(int n)
{
    struct node *new_node,*ptr;
    new_node=(struct node*)malloc(sizeof(struct node));
    new_node->data=n;
    if(START==NULL)
    {
        START=new_node;
        new_node->next=START;
        
    }
    else
    {
        ptr=START;
        while(ptr->next!=START)
            ptr=ptr->next;
        ptr->next=new_node;
        new_node->next=START;
    }
}
void display()
{
    struct node *ptr=START;
    while(ptr->next!=START)
    {
        printf("%d->",ptr->data);
        ptr=ptr->next;
    }
    printf("%d",ptr->data);
}
int main() {
	int a,n;
	scanf("%d",&n);
	while(n--)
	{

	    scanf("%d",&a);
	    create(a);
	}
	display();
	return 0;
}
